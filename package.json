{
  "name": "@hais/ccpretty",
  "version": "1.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ccpretty": "dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./formatters": {
      "import": "./dist/formatters.js",
      "require": "./dist/formatters.js",
      "types": "./dist/formatters.d.ts"
    },
    "./models": {
      "import": "./dist/models.js",
      "require": "./dist/models.js",
      "types": "./dist/models.d.ts"
    },
    "./slack": {
      "import": "./dist/slack.js",
      "require": "./dist/slack.js",
      "types": "./dist/slack.d.ts"
    },
    "./message-queue": {
      "import": "./dist/message-queue.js",
      "require": "./dist/message-queue.js",
      "types": "./dist/message-queue.d.ts"
    },
    "./message-reducer": {
      "import": "./dist/message-reducer.js",
      "require": "./dist/message-reducer.js",
      "types": "./dist/message-reducer.d.ts"
    },
    "./run-with-claude": {
      "import": "./dist/run-with-claude.js",
      "require": "./dist/run-with-claude.js",
      "types": "./dist/run-with-claude.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ccpretty": "node dist/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "log",
    "formatter",
    "json",
    "pretty",
    "cli"
  ],
  "author": "Hais",
  "license": "ISC",
  "description": "A command-line tool to format JSON log lines into human-readable messages",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@axync/extract-json": "^1.1.1",
    "@slack/web-api": "^7.9.2",
    "boxen": "^8.0.1",
    "dotenv": "^16.5.0",
    "picocolors": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hais/ccpretty.git"
  },
  "bugs": {
    "url": "https://github.com/Hais/ccpretty/issues"
  },
  "homepage": "https://github.com/Hais/ccpretty#readme"
}
